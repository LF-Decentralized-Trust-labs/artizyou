{
    "name": "artizyou",
    "private": true,
    "scripts": {
        "build:production": "yarn clean && yarn locales:production && yarn build:production:client && yarn build:production:server",
        "build:production:client": "NODE_ENV=production webpack -p --config webpack/client.config.js",
        "build:production:server": "NODE_ENV=production webpack --config webpack/server.config.js",
        "build:development": "yarn clean && yarn build:development:server && yarn build:development:client",
        "build:development:client": "NODE_ENV=development webpack -w --config webpack/client.config.js",
        "build:development:server": "NODE_ENV=development webpack -w --config webpack/server.config.js",
        "build:test": "yarn clean && yarn build:test:client && yarn build:test:server",
        "build:test:client": "NODE_ENV=test webpack --config webpack/client.config.js",
        "build:test:server": "NODE_ENV=test webpack --config webpack/server.config.js",
        "clean": "rm -rf ../public/js/*",
        "locales": "yaml-to-intl '../config/locales/**/*.yml' -o './locales/index.js' -p",
        "locales:development": "yaml-to-intl '../config/locales/**/*.yml' -o './locales/index.js'",
        "locales:production": "yaml-to-intl '../config/locales/**/*.yml' -o './locales/index.js'"
    },
    "dependencies": {
        "@sentry/react": "^5.27.3",
        "@sentry/tracing": "^5.27.3",
        "babel-core": "^6.26.0",
        "babel-loader": "7.1.2",
        "babel-plugin-universal-import": "^1.3.4",
        "babel-polyfill": "^6.26.0",
        "babel-preset-env": "^1.6.1",
        "babel-preset-react": "^6.24.1",
        "babel-preset-stage-2": "^6.24.1",
        "css-loader": "3.6.0",
        "dotenv-webpack": "^1.7.0",
        "exif-js": "^2.3.0",
        "fungo": "git+ssh://git@github.com:Artizyou-inc/fungo-frontend-library.git",
        "i18n-iso-countries": "^3.4.0",
        "intl": "^1.2.5",
        "lodash": "^4.17.15",
        "npm-run-parallel": "^0.6.0",
        "prop-types": "^15.6.0",
        "react": "^16.2.0",
        "react-burger-menu": "^2.2.3",
        "react-datepicker": "2.9.4",
        "react-dom": "^16.2.0",
        "react-dropdown": "^1.3.6",
        "react-dropzone": "^4.2.6",
        "react-icons": "^4.2.0",
        "react-intl": "^2.4.0",
        "react-modal": "^3.1.11",
        "react-redux": "^5.0.6",
        "react-router-dom": "^4.2.2",
        "react-select": "^1.2.1",
        "react-stripe-elements": "^1.2.1",
        "react-transition-group": "^2.2.1",
        "react-universal-component": "^2.8.0",
        "redux": "^3.7.2",
        "redux-thunk": "^2.2.0",
        "stats-webpack-plugin": "^0.6.1",
        "styled-components": "^2.2.4",
        "svg-react-loader": "^0.4.6",
        "unique-random": "1.0.0",
        "url-loader": "^0.6.2",
        "webpack": "^3.10.0",
        "webpack-flush-chunks": "^1.2.2",
        "whatwg-fetch": "^2.0.3",
        "yaml-to-intl": "git+ssh://git@github.com:Artizyou-inc/yaml-to-intl.git"
